{
    "name": "crisislink-backend",
    "version": "1.0.0",
    "description": "CrisisLink Backend API Server",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.js",
        "dev": "nodemon src/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "migrate": "knex migrate:latest --knexfile config/knexfile.js",
        "migrate:rollback": "knex migrate:rollback --knexfile config/knexfile.js",
        "seed": "knex seed:run --knexfile config/knexfile.js",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "dotenv": "^16.3.1",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "socket.io": "^4.7.4",
        "axios": "^1.6.2",
        "knex": "^3.0.1",
        "pg": "^8.11.3",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "twilio": "^4.19.0",
        "twilio": "^4.19.0",
        "node-cron": "^3.0.3",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "eslint": "^8.55.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1"
    },
    "keywords": [
        "api",
        "express",
        "emergency",
        "disaster-response",
        "postgis"
    ],
    "author": "CrisisLink Team",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0"
    }
}